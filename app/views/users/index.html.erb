<h1>Users</h1>

<table>

<tr>
<th>Id</th>
<th>Name</th>
<th>Email</th>
<th>Created User</th>
<th>Type</th>
<th>Phone</th>
<th>DOB</th>
<th>Address</th>
<th>Operations</th>
</tr>

<tr>
<% @users.each do |user| %>


    <td> <%= user.id %> </td>
    <td> <%= link_to user.name, user %> </td>
    <td> <%= user.email %> </td>
    <td> Admin </td>
    <td> <%= user.role %> </td>
    <td> <%= user.phone %> </td>
    <td> <%= user.dob %> </td>
    <td> <%= user.address %> </td>
    <td>
    <%= link_to "Delete", user_path(user.id),
    method: :delete,
    data: { confirm: "Are you sure?" } %>
    </td>

    </tr>

<% end %>

</table>

<br><br>
<%= link_to "New User", new_user_path %>
